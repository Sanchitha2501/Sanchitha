//Program to parse the input string and print the duplicates.

#include <stdio.h>
#include <string.h>

int main()
{
    char str[100];
    int len, i, j;
    int duplicates[100];
    int count = 0;

    printf("Enter string: ");
    scanf("%s", str);
    len = strlen(str);

    for(i=0; i<len; i++)
    {
        for(j=i+1; j<len; j++)
        {
            if(str[i] == str[j])
            {
                duplicates[count] = str[i];
                count++;
                break;
            }
        }
    }

    if(count == 0)
    {
        printf("No duplicates\n");
    }
    else
    {
        printf("Duplicates : ");
        for(i=0; i<count; i++)
        {
            printf("%c ", duplicates[i]);
        }
        printf("\n");
    }

    return 0;
}
