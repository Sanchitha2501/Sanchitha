//Program to read custom delimiters from and input string.

#include <stdio.h>
#include <string.h>

int main() 
{
    char input[100];
    char delimiter;
    printf("Enter string: ");
    scanf("%[^\n]%*c", input);
    printf("Enter delimiter:");
    scanf("%c", &delimiter);
    
    char *token;
    token = strtok(input, &delimiter);
    while (token != NULL) 
    {
        printf("%s\n", token);
        token = strtok(NULL,  &delimiter);
    }
}
